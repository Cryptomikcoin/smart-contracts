{
  "extends": "solhint:recommended",
  "plugins": [],
  "rules": {
    "avoid-low-level-calls": "off",
    "avoid-suicide": "error",
    "avoid-tx-origin": "off",
    "compiler-version": ["error", "0.6.6"],
    "func-order": "error",
    "max-line-length": ["off", 120],
    "max-states-count": ["warn", 20],
    "not-rely-on-time": "off",
    "no-empty-blocks": "off"
  }
}
